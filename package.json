{
  "name": "news-reader",
  "version": "1.0.0",
  "description": "News Reader App with Express Proxy and React Frontend",
  "scripts": {
    "server:install": "cd server && npm install",
    "web:install": "cd web && npm install",
    "install:all": "npm run server:install && npm run web:install",
    "server:dev": "cd server && npm run dev",
    "web:dev": "cd web && npm run dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run web:dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
